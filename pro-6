create table sailors(
sid number primary key,
sname varchar2(14) not null,
rating number,
age number
);

create table boats(
bid number primary key,
bname varchar2(15) not null,
color varchar2(10)
);

create table reserves(
sid number,
bid number,
day date,
primary key(sid, bid, day),
foreign key(sid) references sailors(sid),
foreign key(bid) references boats(bid)
);

insert into sailors values(1,'John',7,25);
insert into sailors values(2,'Mary',5,30);
insert into sailors values(3,'Alex',8,22);
insert into sailors values(4,'Ravi',6,28);
insert into sailors values(5,'Sita',9,27);

insert into boats values(101,'Ocean Queen','Red');
insert into boats values(102,'Sea Rider','Blue');
insert into boats values(103,'Wave Master','Green');
insert into boats values(104,'Sunset Cruiser','Yellow');
insert into boats values(105,'Storm Runner','Black');

insert into reserves values(1,101,to_date('2025-01-10','yyyy-mm-dd'));
insert into reserves values(1,102,to_date('2025-01-12','yyyy-mm-dd'));
insert into reserves values(2,103,to_date('2025-01-11','yyyy-mm-dd'));
insert into reserves values(3,101,to_date('2025-01-15','yyyy-mm-dd'));
insert into reserves values(4,104,to_date('2025-01-16','yyyy-mm-dd'));

commit;

select * from sailors;
select * from boats;
select * from reserves;

select s.sid, s.sname, b.bname, r.day
from sailors s
inner join reserves r on s.sid=r.sid
inner join boats b on b.bid=r.bid;

select s.sid, s.sname, b.bname, r.day
from sailors s
left join reserves r on s.sid=r.sid
left join boats b on b.bid=r.bid;

select s.sname, b.bname, r.day
from sailors s
right join reserves r on s.sid=r.sid
right join boats b on b.bid=r.bid;

select s.sid, s.sname, b.bid, b.bname, r.day
from sailors s
full outer join reserves r on s.sid=r.sid
full outer join boats b on b.bid=r.bid;

select sid, sname, day
from sailors
natural join reserves;
