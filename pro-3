
select sysdate as date from dual;


select empno, ename, sal, round(sal*1.15,0) as new_salary
from emp;


select ename, initcap(ename) as name_format, length(ename) as name_length
from emp;


select ename, hiredate,
trunc(months_between(sysdate,hiredate)) as months_worked
from emp
order by months_between(sysdate,hiredate);
